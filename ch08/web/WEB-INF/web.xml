<?xml version="1.0" encoding="UTF-8"?><web-app>   <servlet>      <servlet-name>depot-atom</servlet-name>	  <servlet-class>	     com.manning.blogapps.chapter08.web.DepotAtomServlet	  </servlet-class>      <init-param>         <param-name>org.apache.velocity.properties</param-name>         <param-value>/WEB-INF/velocity.properties</param-value>      </init-param>   </servlet>   <servlet>      <servlet-name>depot-newsfeed</servlet-name>	  <servlet-class>	     com.manning.blogapps.chapter08.web.DepotNewsfeedServlet	  </servlet-class>   </servlet>   <servlet>      <servlet-name>depot-newsfeed-cached</servlet-name>	  <servlet-class>	     com.manning.blogapps.chapter08.web.DepotNewsfeedServletCached	  </servlet-class>   </servlet>   <servlet-mapping>      <servlet-name>depot-atom</servlet-name>      <url-pattern>/depot-atom/*</url-pattern>   </servlet-mapping>   <servlet-mapping>      <servlet-name>depot-newsfeed</servlet-name>      <url-pattern>/depot-newsfeed/*</url-pattern>   </servlet-mapping>   <servlet-mapping>      <servlet-name>depot-newsfeed-cached</servlet-name>      <url-pattern>/depot-newsfeed-cached/*</url-pattern>   </servlet-mapping>   <!-- The OSCache cache filter -->   <filter>      <filter-name>CacheFilter</filter-name>      <filter-class>com.opensymphony.oscache.web.filter.CacheFilter</filter-class>      <init-param>         <param-name>time</param-name>         <param-value>600</param-value> <!-- seconds -->      </init-param>      <init-param>         <param-name>scope</param-name>         <param-value>application</param-value>      </init-param>   </filter>      <!-- Our custom Conditional GET filter (unused)   <filter>      <filter-name>ConditionalGetFilter</filter-name>      <filter-class>com.manning.blogapps.chapter08.web.ConditionalGetFilter</filter-class>   </filter>   -->   <!-- Apply the OSCache filter to depot-rss-cached.jsp only -->   <filter-mapping>      <filter-name>CacheFilter</filter-name>      <url-pattern>/depot-rss-cached.jsp</url-pattern>   </filter-mapping>      <!-- Conditional GET filter mapping (unused)   <filter-mapping>       <filter-name>ConditionalGetFilter</filter-name>      <url-pattern>/depot-rss</url-pattern>   </filter-mapping>    --></web-app>